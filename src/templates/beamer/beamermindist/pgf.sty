\def\pgfpicture#1#2#3#4{%
  \setlength\pgf@xa{#3}%
  \setlength\pgf@x{#1}%
  \advance\pgf@xa by-\pgf@x\relax%
  \setlength\pgf@ya{#4}%
  \setlength\pgf@y{#2}%
